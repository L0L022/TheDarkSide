"*":
  "atom-html-preview":
    scrollToCursor: true
  core:
    automaticallyUpdate: false
    openEmptyEditorOnStart: false
    telemetryConsent: "limited"
  "linter-gcc":
    gccIncludePaths: "-"
    gccLintOnTheFly: true
  "linter-shellcheck":
    shellcheckExecutablePath: "~/bin/shellcheck"
  "linter-stylelint":
    disableWhenNoConfig: false
    useStandard: true
  welcome:
    showOnStartup: false
