"*":
  "atom-html-preview":
    scrollToCursor: true
  core:
    automaticallyUpdate: false
    openEmptyEditorOnStart: false
    telemetryConsent: "limited"
  "linter-gcc":
    gccErrorString: "erreur"
    gccIncludePaths: "-"
    gccLintOnTheFly: true
    gccWarningString: "attention"
  "linter-shellcheck":
    enableNotice: true
    shellcheckExecutablePath: "~/bin/shellcheck"
  "linter-stylelint":
    disableWhenNoConfig: false
    useStandard: true
  welcome:
    showOnStartup: false
